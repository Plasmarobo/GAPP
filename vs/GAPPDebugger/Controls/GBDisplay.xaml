<UserControl x:Class="GAPPDebugger.Controls.GBDisplay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="144" d:DesignWidth="160">
    <Grid Background="Gray" Height="144" Width="160">
        <Image x:Name="DisplayFrame" Width="160" Height="144" KeyDown="DisplayFrame_KeyDown" KeyUp="DisplayFrame_KeyUp" Margin="0,0,0,0"></Image>
    </Grid>
</UserControl>
