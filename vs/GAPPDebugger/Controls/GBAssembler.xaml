<Window x:Class="GAPPDebugger.Controls.GBAssembler"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="GBAssembler" Height="800" Width="900">
    <Grid>
        <TextBox x:Name="Assembly" AcceptsReturn="True" Width="400" Margin="10,10,0,10" HorizontalAlignment="Left"/>
        <TextBox x:Name="Execution" IsReadOnly="True" Width="400" Margin="0,10,10,10" HorizontalAlignment="Right"/>
        <Button Content="Load" HorizontalAlignment="Left" Margin="415,689,0,0" VerticalAlignment="Top" Width="62" Click="LoadASM"/>
        <Button Content="Save" HorizontalAlignment="Left" Margin="415,714,0,0" VerticalAlignment="Top" Width="62" Click="SaveASM"/>
        <Button Content="Run" HorizontalAlignment="Left" Margin="415,739,0,0" VerticalAlignment="Top" Width="62" Click="CompileAndRun"/>
    </Grid>
</Window>
