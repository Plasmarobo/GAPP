<Window x:Class="GAPPDebugger.Controls.GBAssembler"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="GBAssembler" Height="800" Width="900">
    <Grid>
        <RichTextBox x:Name="Assembly" Width="400" Margin="10,10,0,10" HorizontalAlignment="Left"/>
        <RichTextBox x:Name="Execution" Width="400" Margin="0,10,10,10" HorizontalAlignment="Right"/>
        <Button Content="Run" HorizontalAlignment="Left" Margin="415,739,0,0" VerticalAlignment="Top" Width="62" Click="CompileAndRun"/>
    </Grid>
</Window>
